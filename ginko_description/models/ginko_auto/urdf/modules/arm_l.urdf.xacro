<?xml version="1.0"?>


<robot name="arm_l" xmlns:xacro="http://www.ros.org/wiki/xacro">
	<xacro:macro name="arm_l" params="name parent">
	<!-- <xacro:property name="name" value="leg"/> -->

		<!-- shoulder pitch. -->
		<link name="${name}_link0">
			<!-- Collision created from STL mesh without color -->
			<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>                                                              
					<mesh filename="package://ginko_description/models/ginko_auto/meshes/stl/link09_l_arm01.stl"/>
				</geometry>
			</collision>
			<!-- Visualisation created from Color dae -->
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://ginko_description/models/ginko_auto/meshes/dae/link09_l_arm01.dae"/>
				</geometry>
			</visual>
			<!-- nothing worth -->
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="6.1"/>
				<inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
			</inertial>
		</link>
		<joint name="${name}_joint0" type="revolute">
		    <parent link="${parent}"/>
			<child link="${name}_link0"/>
		    <origin xyz="0 0 0" rpy="0 0.3927 0"/>
		    <axis xyz="0 -1 0"/>
			<limit effort="1000.0" lower="-2.6" upper="2.6" velocity="0.5"/>
		</joint>


		<!-- shoulder roll -->
		<link name="${name}_link1">
			<!-- Collision created from STL mesh without color -->
			<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>                                                              
					<mesh filename="package://ginko_description/models/ginko_auto/meshes/stl/link10_l_arm02.stl"/>
	    		</geometry>
			</collision>
			<!-- Visualisation created from Color dae -->
	    	<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>
					<mesh filename="package://ginko_description/models/ginko_auto/meshes/dae/link10_l_arm02.dae"/>
	    		</geometry>
			</visual>
			<!-- nothing worth -->
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="6.1"/>
				<inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
			</inertial>
		</link>
		<joint name="${name}_joint1" type="revolute">
			<parent link="${name}_link0"/>
			<child  link="${name}_link1"/>
			<origin xyz="0.035 0.0246 0" rpy="1.1781 0 0"/>
			<axis xyz="1 0 0"/>
			<limit effort="1000.0" lower="-0.85" upper="1.18" velocity="0.5"/>
		</joint>
		<link name="${name}_link1_rev"/>
		<joint name="${name}_joint1_rev" type="revolute">
			<parent link="${name}_link0"/>
			<child  link="${name}_link1_rev"/>
			<origin xyz="-0.0562 0.0246 0" rpy="-1.1781 0 3.1416"/>
			<axis xyz="1 0 0"/>
	    	<limit effort="1000.0" lower="-3.141593" upper="3.141593" velocity="0.5"/>
	    	<!-- <mimic joint="${name}_joint1" multiplier="-1" offset="0"/>  -->
		</joint>



		<!-- scissors -->
		<link name="${name}_link2">
			<!-- Collision created from STL mesh without color -->
	    	<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>                                                              
					<mesh filename="package://ginko_description/models/ginko_auto/meshes/stl/link11_l_scissor01.stl"/>
	    		</geometry>
			</collision>
			<!-- Visualisation created from Color dae -->
	    	<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>
					<mesh filename="package://ginko_description/models/ginko_auto/meshes/dae/link11_l_scissor01.dae"/>
	    		</geometry>
			</visual>
			<!-- nothing worth -->
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="6.1"/>
				<inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
			</inertial>
		</link>
		<joint name="${name}_joint2" type="revolute">
	    	<parent link="${name}_link1"/>
	    	<child  link="${name}_link2"/>
	    	<origin xyz="0.0092 0.0547 -0.11295" rpy="0 0.7854 0"/>
	    	<axis xyz="0 1 0"/>
	    	<limit effort="1000.0" lower="-2.36" upper="2.36" velocity="0.5"/>
	  	</joint>

		<link name="${name}_link3">
			<!-- Collision created from STL mesh without color -->
	    	<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>                                                              
					<mesh filename="package://ginko_description/models/ginko_auto/meshes/stl/link12_l_scissor02.stl"/>
	    		</geometry>
			</collision>
			<!-- Visualisation created from Color dae -->
	    	<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>
					<mesh filename="package://ginko_description/models/ginko_auto/meshes/dae/link12_l_scissor02.dae"/>
	    		</geometry>
			</visual>
			<!-- nothing worth -->
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="6.1"/>
				<inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
			</inertial>
		</link>
		<joint name="${name}_joint3" type="revolute">
	    	<parent link="${name}_link1"/>
	    	<child  link="${name}_link3"/>
	    	<origin xyz="0.0092 -0.0018 -0.11295" rpy="0 0.7854 0"/>
	    	<axis xyz="0 1 0"/>
	    	<limit effort="1000.0" lower="-2.36" upper="2.36" velocity="0.5"/>
	  	</joint>

	  	<!-- LiDAR -->
		<link name="${name}_urg_link">
			<!-- Visualisation created from Color dae -->
	    	<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>
					<mesh filename="package://ginko_description/models/ginko_auto/meshes/dae/urg.dae"/>
	    		</geometry>
			</visual>
			<!-- nothing worth -->
			<inertial>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<mass value="6.1"/>
				<inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
			</inertial>
		</link>
		<joint name="${name}_urg_joint" type="fixed">
	    	<parent link="${name}_link1"/>
	    	<child  link="${name}_urg_link"/>
	    	<origin xyz="-0.0456 -0.01715 -0.06795" rpy="0 1.57 -1.57"/>
	  	</joint>


	</xacro:macro>
</robot>
