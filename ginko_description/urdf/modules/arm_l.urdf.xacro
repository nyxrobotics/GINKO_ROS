<?xml version="1.0"?>


<robot name="arm_l" xmlns:xacro="http://www.ros.org/wiki/xacro">
	<xacro:macro name="arm_l" params="name parent">
	<!-- <xacro:property name="name" value="leg"/> -->

		<!-- shoulder pitch. -->
		<link name="${name}_link0">
			<!-- Collision created from STL mesh without color -->
			<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<!-- <mesh filename="package://ginko_description/meshes/stl/link01_waist.stl"/> -->
					<mesh filename="package://ginko_description/meshes/stl/link09_l_arm01.stl"/>
				</geometry>
			</collision>
			<!-- Visualisation created from Color dae -->
			<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
				<geometry>
					<!-- <mesh filename="package://ginko_description/meshes/stl/link01_waist.stl"/> -->
					<mesh filename="package://ginko_description/meshes/stl/link09_l_arm01.stl"/>
					<!-- <mesh filename="package://ginko_description/meshes/dae/link09_l_arm01.dae"/> -->
				</geometry>
			</visual>
			<!-- nothing worth -->
			<inertial>
				<mass value="0.01356014"/>
				<origin xyz="-0.01125790 0.00529265 0.00000000" rpy="0 0 0"/>
				<inertia
					ixx="0.00000235"
					ixy="0.00000004" iyy="0.00001670"
					ixz="0.00000000" iyz="0.00000000" izz="0.00001606"/>
			</inertial>
		</link>
		<joint name="${name}_joint0" type="revolute">
		    <parent link="${parent}"/>
			<child link="${name}_link0"/>
		    <origin xyz="0 0.10683 0.05115" rpy="0 0.3927 0"/>
		    <axis xyz="0 -1 0"/>
			<limit effort="1000.0" lower="-2.6" upper="2.6" velocity="0.5"/>
		</joint>


		<!-- shoulder roll -->
		<link name="${name}_link1">
			<!-- Collision created from STL mesh without color -->
			<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>                                                              
					<mesh filename="package://ginko_description/meshes/stl/link10_l_arm02.stl"/>
	    		</geometry>
			</collision>
			<!-- Visualisation created from Color dae -->
	    	<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>
					<mesh filename="package://ginko_description/meshes/stl/link10_l_arm02.stl"/>
					<!-- <mesh filename="package://ginko_description/meshes/dae/link10_l_arm02.dae"/> -->
	    		</geometry>
			</visual>
			<!-- nothing worth -->
			<inertial>
				<mass value="0.33139947"/>
				<origin xyz="-0.02612786 0.02650564 -0.06333369" rpy="0 0 0"/>
				<inertia
					ixx="0.00115128"
					ixy="0.00015366" iyy="0.00113572"
					ixz="-0.00031848" iyz="-0.00037104" izz="0.00064743"/>
			</inertial>
		</link>
		<joint name="${name}_joint1" type="revolute">
			<parent link="${name}_link0"/>
			<child  link="${name}_link1"/>
			<origin xyz="0.035 0.0246 0" rpy="1.1781 0 0"/>
			<axis xyz="1 0 0"/>
			<limit effort="1000.0" lower="-0.85" upper="1.18" velocity="0.5"/>
		</joint>
		<link name="${name}_link1_rev"/>
		<joint name="${name}_joint1_rev" type="revolute">
			<parent link="${name}_link0"/>
			<child  link="${name}_link1_rev"/>
			<origin xyz="-0.0562 0.0246 0" rpy="-1.1781 0 3.1416"/>
			<axis xyz="1 0 0"/>
	    	<limit effort="1000.0" lower="-3.141593" upper="3.141593" velocity="0.5"/>
	    	<!-- <mimic joint="${name}_joint1" multiplier="-1" offset="0"/>  -->
		</joint>



		<!-- scissors -->
		<link name="${name}_link2">
			<!-- Collision created from STL mesh without color -->
	    	<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>                                                              
					<mesh filename="package://ginko_description/meshes/stl/link11_l_scissor01.stl"/>
	    		</geometry>
			</collision>
			<!-- Visualisation created from Color dae -->
	    	<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>
					<mesh filename="package://ginko_description/meshes/stl/link11_l_scissor01.stl"/>
					<!-- <mesh filename="package://ginko_description/meshes/dae/link11_l_scissor01.dae"/> -->
	    		</geometry>
			</visual>
			<!-- nothing worth -->
			<inertial>
				<mass value="0.03287850"/>
				<origin xyz="0.08234859 -0.030246866 0.00514112" rpy="0 0 0"/>
				<inertia
					ixx="0.00002277"
					ixy="-0.00000014" iyy="0.00008197"
					ixz="-0.00000058" iyz="-0.00000001" izz="0.00010105"/>
			</inertial>
		</link>
		<joint name="${name}_joint2" type="revolute">
	    	<parent link="${name}_link1"/>
	    	<child  link="${name}_link2"/>
	    	<origin xyz="0.0092 0.0547 -0.11295" rpy="0 0.7854 0"/>
	    	<axis xyz="0 1 0"/>
	    	<limit effort="1000.0" lower="-2.36" upper="2.36" velocity="0.5"/>
	  	</joint>

		<link name="${name}_link3">
			<!-- Collision created from STL mesh without color -->
	    	<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>                                                              
					<mesh filename="package://ginko_description/meshes/stl/link12_l_scissor02.stl"/>
	    		</geometry>
			</collision>
			<!-- Visualisation created from Color dae -->
	    	<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>
					<mesh filename="package://ginko_description/meshes/stl/link12_l_scissor02.stl"/>
					<!-- <mesh filename="package://ginko_description/meshes/dae/link12_l_scissor02.dae"/> -->
	    		</geometry>
			</visual>
			<!-- nothing worth -->
			<inertial>
				<mass value="0.02936110"/>
				<origin xyz="0.07807033 0.02515230 -0.00665437" rpy="0 0 0"/>
				<inertia
					ixx="0.00001452"
					ixy="0.00000023" iyy="0.00006407"
					ixz="-0.00000245" iyz="-0.00000002" izz="0.00008024"/>
			</inertial>
		</link>
		<joint name="${name}_joint3" type="revolute">
	    	<parent link="${name}_link1"/>
	    	<child  link="${name}_link3"/>
	    	<origin xyz="0.0092 -0.0018 -0.11295" rpy="0 0.7854 0"/>
	    	<axis xyz="0 1 0"/>
	    	<limit effort="1000.0" lower="-2.36" upper="2.36" velocity="0.5"/>
	  	</joint>

	  	<!-- LiDAR -->
		<link name="${name}_urg_link">
	    	<collision>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>                                                              
					<mesh filename="package://ginko_description/meshes/stl/urg.stl"/>
	    		</geometry>
			</collision>
			<!-- Visualisation created from Color dae -->
	    	<visual>
				<origin xyz="0 0 0" rpy="0 0 0"/>
	    		<geometry>
	    			<mesh filename="package://ginko_description/meshes/stl/urg.stl"/>
					<!-- <mesh filename="package://ginko_description/meshes/dae/urg.dae"/> -->
	    		</geometry>
			</visual>
			<!-- nothing worth -->
			<inertial>
				<mass value="0.16705492"/>
				<origin xyz="-0.00014579 0.00000831 -0.02360505" rpy="0 0 0"/>
				<inertia
					ixx="0.00008801"
					ixy="0.00000000" iyy="0.00008854"
					ixz="-0.00000049" iyz="-0.00000004" izz="0.00006019"/>
			</inertial>
		</link>
		<joint name="${name}_urg_joint" type="fixed">
	    	<parent link="${name}_link1"/>
	    	<child  link="${name}_urg_link"/>
	    	<origin xyz="-0.0456 -0.01715 -0.06795" rpy="0 1.57 -1.57"/>
	  	</joint>


	</xacro:macro>
</robot>
